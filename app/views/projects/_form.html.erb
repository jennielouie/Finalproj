<%= form_for (@project) do |f| %>
  <div class = "form-inputs">
    <div class = "field">
      <%= f.label "Project Title" %></br>
      <%= f.text_field :projtitle %>
    </div>

    <div class = "field">
      <%= f.label "Project Author" %></br>
      <%= f.text_field :author %>
    </div>

    <div class = "field">
      <%= f.label "Project Notes" %></br>
      <%= f.text_area :notes %>
    </div>

    <div class = "field">
      <%= f.label "Pattern File" %></br>
      <%= f.file_field :patternFile %>
    </div>
  </div>
  <div class = "form-actions">
    <%= f.submit %>
  </div>
<% end %>